{
  "environments": {
    "replaced_name": {
      "machines": {
        "dev-machine": {
          "servers": {
            "tomcat8" : {
              "port" : "8080",
              "protocol" : "http"
            },
            "tomcat8-debug" : {
              "port" : "8000",
              "protocol" : "http"
            }
          },
          "installers": [
            "org.eclipse.che.terminal",
            "org.eclipse.che.ws-agent",
            "org.eclipse.che.ls.python",
            "org.eclipse.che.exec"
          ],
          "attributes": {
            "memoryLimitBytes": "desired_memory_value"
          }
        }
      },
      "recipe": {
        "content": "eclipse/ubuntu_python:latest",
        "type": "dockerimage"
      }
    }
  },
  "defaultEnv": "replaced_name",
  "name": "replaced_name"
}
